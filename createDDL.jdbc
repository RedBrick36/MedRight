CREATE TABLE APP.TREATMENTS (UUID INTEGER NOT NULL, AFTERNOON SMALLINT DEFAULT 0, ALLDAYS SMALLINT DEFAULT 0, ALLTIMES SMALLINT DEFAULT 0, AM SMALLINT DEFAULT 0, ASNEEDED SMALLINT DEFAULT 0, BEDTIME SMALLINT DEFAULT 0, CONDITION VARCHAR(45), DOSE FLOAT, EVENING SMALLINT DEFAULT 0, FRIDAY SMALLINT DEFAULT 0, LEADTIME INTEGER, MEASURE VARCHAR(11), MIDAFT SMALLINT DEFAULT 0, MIDAM SMALLINT DEFAULT 0, MIDOFNIGHT SMALLINT DEFAULT 0, MONDAY SMALLINT DEFAULT 0, NAME VARCHAR(40), NOON SMALLINT DEFAULT 0, OTF VARCHAR(20), REMINDER SMALLINT DEFAULT 0, SATURDAY SMALLINT DEFAULT 0, SUNDAY SMALLINT DEFAULT 0, THURSDAY SMALLINT DEFAULT 0, TUESDAY SMALLINT DEFAULT 0, TYPE VARCHAR(10), WEDNESDAY SMALLINT DEFAULT 0, PRIMARY KEY (UUID))
